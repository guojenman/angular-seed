{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "target": "es5",
    "module": "es2015",
    "removeComments": true
  },
  "angularCompilerOptions": {
    "skipMetadataEmit": true
  },
  "exclude": [
    "**/*.aot.ts"
  ]
}